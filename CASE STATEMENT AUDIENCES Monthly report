CASE
WHEN CONTAINS_TEXT(Ad set name,"160821") AND (CONTAINS_TEXT(Ad set name,"Non-Lloyds") OR CONTAINS_TEXT(Ad set name,"Non Lloyds"))  THEN "Broad Audience - Non-Lloyds territories - Aug-21"
WHEN CONTAINS_TEXT(Ad set name,"160821")  AND (CONTAINS_TEXT(Ad set name,"| Lloyds") OR CONTAINS_TEXT(Ad set name,"Broad Lloyds")) THEN "Broad Audience - Lloyds territories - Aug-21"
WHEN CONTAINS_TEXT(Ad set name,"Non-Lloyds") OR CONTAINS_TEXT(Ad set name,"Non Lloyds") THEN "Broad Audience - Non-Lloyds territories"
WHEN CONTAINS_TEXT(Ad set name,"Broad Lloyds") OR CONTAINS_TEXT(Ad set name,"| Lloyds")  THEN "Broad Audience - Lloyds territories"
WHEN CONTAINS_TEXT(Ad set name,"Broad") THEN "Broad Audience "
WHEN CONTAINS_TEXT(Ad set name,"LAL1%CRM") OR CONTAINS_TEXT(Ad set name,"LAL 1% CRM") THEN "LAL 1% CRM"
WHEN CONTAINS_TEXT(Ad set name,"LAL5%CRM") OR CONTAINS_TEXT(Ad set name,"LAL 5% CRM") THEN "LAL 5% CRM"
WHEN CONTAINS_TEXT(Ad set name,"LAL10%CRM") OR CONTAINS_TEXT(Ad set name,"LAL 10% CRM") THEN "LAL 10% CRM"
WHEN CONTAINS_TEXT(Ad set name,"LAL1%REG 30D") OR CONTAINS_TEXT(Ad set name,"LAL1%REG30D") THEN "LAL 1% 30 Days registrations"
WHEN CONTAINS_TEXT(Ad set name,"LAL3%REG 30D") OR CONTAINS_TEXT(Ad set name,"LAL3%REG30D") THEN "LAL 3% 30 Days registrations"
WHEN CONTAINS_TEXT(Ad set name,"LAL1% of 25% most active users 30d") THEN "LAL1% of 25% most active users 30d"
WHEN CONTAINS_TEXT(Ad set name,"LAL1% of 10% most active users 90d") THEN "LAL1% of 10% most active users 90d"
WHEN CONTAINS_TEXT(Ad set name,"LAL1%App launches 30d") THEN "LAL1% of App launches 30d"
WHEN CONTAINS_TEXT(Ad set name,"LAL10% Complete Registrations") THEN "LAL10% Complete Registrations"
WHEN CONTAINS_TEXT(Ad set name,"WV 7D") THEN "7 days website visitors"
WHEN CONTAINS_TEXT(Ad set name,"WV 7-14D") THEN "7-14 days website visitors"
WHEN CONTAINS_TEXT(Ad set name,"WV 30D") THEN "30 days website visitors"
WHEN CONTAINS_TEXT(Ad set name,"VV 95") OR CONTAINS_TEXT(Ad set name,"VV95") THEN "95% video views"
WHEN CONTAINS_TEXT(Ad set name,"Moving House") THEN "Behaviour - Moving Houses"
WHEN CONTAINS_TEXT(Ad set name,"Interest") THEN "Interest targeting"
WHEN CONTAINS_TEXT(Ad set name,"Carers") THEN "Carers"
WHEN CONTAINS_TEXT(Ad set name,"page - Women") THEN "Demographic - Women"
WHEN CONTAINS_TEXT(Ad set name,"page - Men") THEN "Demographic - Men"
WHEN CONTAINS_TEXT(Ad set name,"age+Bone") THEN "Interest - Age&Bone Health Issues"
WHEN CONTAINS_TEXT(Ad set name,"365 Days Social Engagers") THEN "365 Days Social Engagers"
Else "Else"
End
